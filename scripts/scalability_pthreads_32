#!/bin/bash
#SBATCH --job-name=fluidanimate_scalability_pthreads
#SBATCH --time=0-0:1
#SBATCH --cpus-per-task=32
#SBATCH --hint=compute_bound
#SBATCH --mail-user=cadubentzen@gmail.com
#SBATCH --mail-type=ALL

for i in 1 2 3
do
	parsecmgmt -a run -p parsec.fluidanimate -c gcc-pthreads -i simlarge -n 32
	parsecmgmt -a run -p parsec.fluidanimate -c gcc-pthreads -i native -n 32
done
